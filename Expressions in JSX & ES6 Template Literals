import React from "react";
import ReactDOM from "react-dom";

const name = "Swayam"
const number = "9"

ReactDOM.render(
<div>
<h1>Hello {name}</h1>
<p>My lucky number is {number}</p>
<p>Vivek's lucky number is {Math.floor(Math.random()*10)}</p>
<p>Akhilesh's lucky number is {number}</p>
</div>,
document.getElementById("root"));
